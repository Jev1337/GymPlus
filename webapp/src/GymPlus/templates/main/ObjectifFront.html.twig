							</div>
						</div>
					
					

						<div class="col-lg-6">
							<div class="contact-area1 style-1 m-r20 m-md-r0 wow fadeInUp" data-wow-delay="0.5s">
								<div class="dzFormMsg"></div>
									{{ form_start(form, {'attr': {'class': 'dz-form dzForm', 'id': 'objectif'} }) }}

									<div class="form-group">
										<label class="form-label">Weight Objectif</label>
										<div class="input-group">
										{{ form_widget(form.poidsobj, {'attr': {'class': 'form-control', 'placeholder': 'Weight Objectif'}}) }}
										</div>
										<label class="form-label">Weight Actuelle</label>
										<div class="input-group">
										{{ form_widget(form.poidsact, {'attr': {'class': 'form-control', 'placeholder': 'Weight Actuelle'}}) }}
										</div>
										<label class="form-label">Height</label>
										<div class="input-group">
										{{ form_widget(form.taille, {'attr': {'class': 'form-control', 'placeholder': 'Height'}}) }}
										</div>
										<label class="form-label">Something To Say*</label>
										<div class="input-group">
										{{ form_widget(form.alergie, {'attr': {'class': 'form-control', 'placeholder': 'Something To Say*'}}) }}
										</div>
									
										<label class="form-label" for="{{ form.typeobj.vars.id }}">Type Objectif</label>
										<div class="input-group">
										{{ form_widget(form.typeobj, {'attr': {'class': 'form-control', 'placeholder': '', 'id': form.typeobj.vars.id}}) }}
										</div>
										<label class="form-label">Date</label>
										<div class="input-group">
										{{ form_widget(form.datef, {'attr': {'class': 'form-control', 'placeholder': 'Date of Birth'}}) }}
										</div>
										<label class="form-label">Coach</label>
										<div class="input-group">
										{{ form_widget(form.coachid)}}
										</div>
										<div class="text-center">
										{{ form_widget(form.submit, {'attr': {'class': 'btn btn-primary w-100 btnhover', 'value': 'Add Objectif'} } ) }}
										</div>	
									</div>
									{{ form_end(form) }}
									
								</div>
								
							</div>
							
						</div>
						
					</div>
				</div>
			</div>

		<div class="content-inner-2 pt-0">
			<div class="map-iframe style-1">
				<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d227748.3825624477!2d75.65046970649679!3d26.88544791796718!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x396c4adf4c57e281%3A0xce1c63a0cf22e09!2sJaipur%2C+Rajasthan!5e0!3m2!1sen!2sin!4v1500819483219" style="border:0; width:100%; min-height:100%; margin-bottom: -8px;" allowfullscreen=""></iframe>
